.progress_section{
  width: 100%;
  height: 25px;
  margin-bottom: 30px;
}

.btn1{
  height: 40px;
  color: white;
  border-radius: 98rpx;
  background-color: burlywood;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}


.btn1 image{
  width: 20px;
  height: 20px;
}

.weui-cells{
  margin-top: 8px;
  line-height: 1.41176471;
  font-size: 17px;
  overflow: hidden;
  position: relative;
}

.weui-cell{
  padding:16px;
  position: relative;
  display: flex;
  align-items: center;
  border-bottom: black dashed 1rpx;
}

.weui-cell:active{
  background-color: rgba(128, 128, 128, 0.39);
}
